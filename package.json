{
    "name": "sdk-generator",
    "version": "1.0.0",
    "description": "SDK generator based on a weclapp api swagger file",
    "main": "./src/index.ts",
    "author": "weclapp",
    "private": true,
    "scripts": {
        "gen:start": "ts-node -r tsconfig-paths/register src/index.ts --project tsconfig.json",
        "gen:dev": "ts-node-dev --respawn --watch static/**/* -r tsconfig-paths/register src/index.ts",
        "sdk:build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
        "sdk:dev": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch",
        "build": "npm run gen:start && npm run sdk:build",
        "lint:ts": "eslint {src,tests}/**/*.ts",
        "lint:fix": "npm run lint:ts -- --fix",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:dev": "jest --watch",
        "openapi": "swagger2openapi"
    },
    "repository": {
        "type": "git",
        "url": "git@git.internal.weclapp.com:weclapp/weclapp-api/sdk-generator.git"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.22",
        "@types/node-fetch": "^2.5.8",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "@wessberg/rollup-plugin-ts": "^1.3.8",
        "cross-env": "^7.0.3",
        "eslint": "^7.18.0",
        "jest": "^26.6.3",
        "jest-extended": "^0.11.5",
        "joi": "^17.3.0",
        "node-fetch": "^2.6.1",
        "openapi-types": "^7.2.3",
        "rollup": "^2.37.1",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-terser": "^7.0.2",
        "rxjs": "^6.6.3",
        "swagger2openapi": "^7.0.5",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.1",
        "tsconfig-paths": "^3.9.0"
    },
    "dependencies": {
        "@types/fs-extra": "^9.0.6",
        "chalk": "^4.1.0",
        "change-case": "^4.1.2",
        "dotenv": "^8.2.0",
        "fs-extra": "^9.1.0",
        "indent-string": "^4.0.0",
        "typescript": "^4.1.3"
    }
}
