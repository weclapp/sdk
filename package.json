{
    "name": "sdk-generator",
    "version": "0.0.0",
    "description": "SDK generator based on a weclapp api swagger file",
    "main": "./src/index.ts",
    "author": "weclapp",
    "private": true,
    "scripts": {
        "gen:start": "ts-node -r tsconfig-paths/register src/index.ts --project tsconfig.json",
        "gen:dev": "ts-node-dev --respawn --watch \"static/**/*\" -r tsconfig-paths/register src/index.ts",
        "sdk:build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
        "sdk:dev": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch",
        "start": "npm run gen:start && npm run sdk:build",
        "lint": "eslint {src,tests}/**/*.ts",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:dev": "jest --watch",
        "import:swagger": "ts-node scripts/import-swagger.ts --project tsconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "git@git.internal.weclapp.com:weclapp/weclapp-api/sdk-generator.git"
    },
    "devDependencies": {
        "@types/jest": "^27.0.1",
        "@types/node": "^15.14.9",
        "@types/node-fetch": "^2.5.12",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "rollup-plugin-ts": "^1.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "jest": "^27.1.0",
        "jest-extended": "^0.11.5",
        "joi": "^17.4.2",
        "node-fetch": "^2.6.1",
        "openapi-types": "^9.2.0",
        "rollup": "^2.56.3",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-terser": "^7.0.2",
        "rxjs": "^7.3.0",
        "swagger2openapi": "^7.0.8",
        "ts-jest": "^27.0.5",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0"
    },
    "dependencies": {
        "@types/fs-extra": "^9.0.12",
        "chalk": "^4.1.2",
        "change-case": "^4.1.2",
        "dotenv": "^10.0.0",
        "fs-extra": "^10.0.0",
        "indent-string": "^4.0.0",
        "typescript": "^4.4.2"
    }
}
