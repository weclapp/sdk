{
    "name": "sdk-generator",
    "version": "1.0.0",
    "description": "SDK generated based on a weclapp api swagger file",
    "main": "src/index.ts",
    "author": "weclapp",
    "scripts": {
        "gen:start": "ts-node src/index.ts --project tsconfig.json",
        "gen:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "npm run lint -- --fix"
    },
    "repository": {
        "type": "git",
        "url": "git@git.internal.weclapp.com:weclapp/weclapp-api/sdk-generator.git"
    },
    "devDependencies": {
        "@types/node": "^14.14.19",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "eslint": "^7.17.0",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.1",
        "typescript": "^4.1.3"
    }
}
