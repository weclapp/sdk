{
  "name": "@weclapp/sdk",
  "version": "0.3.0",
  "description": "SDK generator based on a weclapp api swagger file",
  "main": "./main/index.js",
  "module": "./main/index.mjs",
  "types": "./main/index.d.ts",
  "author": "weclapp",
  "bin": {
    "sdk-generator": "./bin/cli.js"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "static",
    "rollup.config.js",
    "tsconfig.json",
    "tsconfig.lib.json"
  ],
  "publishConfig": {
    "@weclapp:registry": "https://gitlab.internal.weclapp.com/api/v4/projects/145/packages/npm/"
  },
  "scripts": {
    "cli:build": "cross-env NODE_ENV=production rollup -c rollup.config.js",
    "cli:watch": "cross-env NODE_ENV=development rollup -c rollup.config.js --watch",
    "sdk:build": "./bin/cli.js --from-env --include-hidden",
    "lint": "eslint {src,tests}/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --watch",
    "postinstall": "npm run cli:build"
  },
  "repository": {
    "type": "git",
    "url": "git@git.internal.weclapp.com:weclapp/weclapp-api/sdk-generator.git"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "jest": "^27.4.7",
    "jest-extended": "^2.0.0",
    "joi": "^17.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.12",
    "@types/fs-extra": "^9.0.13",
    "@types/yargs": "^17.0.8",
    "@types/node": "^16.11.22",
    "@rollup/plugin-json": "^4.1.0",
    "cross-env": "^7.0.3",
    "swagger2openapi": "^7.0.8",
    "openapi-types": "^10.0.0",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "dotenv": "^15.0.0",
    "rxjs": "^7.5.2",
    "fs-extra": "^10.0.0",
    "indent-string": "^4.0.0",
    "node-fetch": "^2.6.7",
    "rollup": "^2.66.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.5",
    "typescript": "^4.5.5",
    "yargs": "^17.3.1"
  }
}
