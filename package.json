{
    "name": "sdk-generator",
    "version": "1.0.0",
    "description": "SDK generated based on a weclapp api swagger file",
    "main": "src/index.ts",
    "author": "weclapp",
    "scripts": {
        "gen:start": "ts-node -r tsconfig-paths/register src/index.ts --project tsconfig.json",
        "gen:dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
        "lint:ts": "eslint src/**/*.ts",
        "lint:fix": "npm run lint:ts -- --fix"
    },
    "repository": {
        "type": "git",
        "url": "git@git.internal.weclapp.com:weclapp/weclapp-api/sdk-generator.git"
    },
    "devDependencies": {
        "@types/node": "^14.14.19",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "eslint": "^7.17.0",
        "openapi-types": "^7.2.2",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.1",
        "tsconfig-paths": "^3.9.0"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "change-case": "^4.1.2",
        "dotenv": "^8.2.0",
        "indent-string": "^4.0.0",
        "strip-indent": "^3.0.0",
        "typescript": "^4.1.3"
    }
}
